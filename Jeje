sudo touch co1 co2 co3
sudo nano co1
sudo chmod -R 777 /STUDENT
echo jedna a jedna >> co1
tail -2 co1
sudo touch co1 co2 co3
sudo cp co2 ~/Plocha
ls -l > ~/Plocha/zk3
cd ~/Plocha
